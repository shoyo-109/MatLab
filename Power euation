%First Power equation(ab^x)
clc;
clear all;
close all;
x=input('Enter the value of x');
y=input('Enter the value of y');
n=input('Enter the value of n');
Y=log(y);
sx=0;
sY=0;
sxY=0;
sx2=0;
for i=1:n
sx=sx+x(i);
sY=sY+Y(i);
sxY=sxY+x(i)*Y(i);
sx2=sx2+x(i)*x(i);
end
p=[sx,n;sx2,sx];
q=[sY;sxY];
ans=inv(p)*(q);
A=ans(1);
B=ans(2);
fprintf('\na=%f\nb=%f',exp(B),exp(A));
fprintf('\ny=(%0.4f)*(%0.4f)^x',exp(B),exp(A));



polyfit(x,Y,1)
a=exp(0.1822)
b=exp(4.6054)
